{
  "name": "storefront",
  "version": "1.0.4",
  "private": true,
  "author": "github.com/abbesm0hamed",
  "description": "complete ecommerce app for tunisians and international businesses",
  "keywords": [
    "ecommerce",
    "storefront",
    "store",
    "shop"
  ],
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 8000",
    "dev:turbo": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 8000 --turbopack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 8000",
    "analyze": "ANALYZE=true next build",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "bump-deps": "npx npm-check-updates --deep -u -x react-day-picker,tailwindcss && pnpm install",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.726.1",
    "@headlessui/react": "^2.2.8",
    "@medusajs/icons": "latest",
    "@medusajs/js-sdk": "latest",
    "@medusajs/ui": "latest",
    "@payloadcms/admin-bar": "^3.55.1",
    "@payloadcms/db-postgres": "^3.55.1",
    "@payloadcms/email-nodemailer": "^3.55.1",
    "@payloadcms/live-preview-react": "^3.55.1",
    "@payloadcms/next": "^3.55.1",
    "@payloadcms/payload-cloud": "^3.55.1",
    "@payloadcms/plugin-form-builder": "^3.55.1",
    "@payloadcms/plugin-nested-docs": "^3.55.1",
    "@payloadcms/plugin-redirects": "^3.55.1",
    "@payloadcms/plugin-search": "^3.55.1",
    "@payloadcms/plugin-seo": "^3.55.1",
    "@payloadcms/richtext-lexical": "^3.55.1",
    "@payloadcms/storage-s3": "^3.55.1",
    "@payloadcms/storage-uploadthing": "^3.55.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@repo/seo": "workspace:*",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "algoliasearch": "^5.37.0",
    "class-variance-authority": "^0.7.1",
    "cross-env": "^10.0.0",
    "geist": "^1.5.1",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "next": "^15.5.3",
    "next-intl": "^4.3.9",
    "nodemailer": "^7.0.6",
    "payload": "^3.55.1",
    "pg": "^8.16.3",
    "qs": "^6.14.0",
    "react": "19.1.1",
    "react-country-flag": "^3.1.0",
    "react-dom": "19.1.1",
    "react-instantsearch": "^7.16.3",
    "server-only": "^0.0.1",
    "sharp": "^0.34.3",
    "tailwindcss-radix": "^4.0.2",
    "types": "link:@payloadcms/plugin-seo/types",
    "webpack": "^5",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@medusajs/types": "latest",
    "@medusajs/ui-preset": "latest",
    "@types/lodash": "^4.17.20",
    "@types/node": "24.4.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-instantsearch-dom": "^6.12.9",
    "ansi-colors": "^4.1.3",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "eslint": "9.35.0",
    "eslint-config-next": "15.5.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "overrides": {
      "@aws-sdk/client-s3": "3.726.1",
      "undici": "5.20.0"
    }
  },
